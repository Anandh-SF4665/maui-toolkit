<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Syncfusion.Maui.Toolkit.Expander.SfExpanderStyles"
             xmlns:expander="clr-namespace:Syncfusion.Maui.Toolkit.Expander">
  <Style TargetType="expander:SfExpander">
        <Setter Property="HeaderBackground"
                Value="{DynamicResource SfExpanderNormalHeaderBackground}" />
        <Setter Property="HeaderIconColor"
                Value="{DynamicResource SfExpanderNormalHeaderIconColor}" />
    </Style>
</ResourceDictionary>