<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Syncfusion.Maui.Toolkit.Accordion.SfAccordionStyles"
             xmlns:accordion="clr-namespace:Syncfusion.Maui.Toolkit.Accordion">
  <Style TargetType="accordion:AccordionItem">
        <Setter Property="HeaderBackground"
                Value="{DynamicResource SfAccordionNormalHeaderBackground}" />
        <Setter Property="HeaderIconColor"
                Value="{DynamicResource SfAccordionNormalHeaderIconColor}" />
    </Style>
</ResourceDictionary>